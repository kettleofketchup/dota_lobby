# Example .env file for dota_lobby
# Copy this to .env and fill in your actual values
# NEVER commit the .env file with real credentials!

# Server Configuration
DOTA_LOBBY_SERVER_HOST=0.0.0.0
DOTA_LOBBY_SERVER_PORT=8080

# Steam Configuration
DOTA_LOBBY_STEAM_API_KEY=your_steam_api_key_here

# Bot 1 Configuration
DOTA_LOBBY_BOTS_0_NAME=bot1
DOTA_LOBBY_BOTS_0_USERNAME=your_steam_username_1
DOTA_LOBBY_BOTS_0_PASSWORD=your_steam_password_1
DOTA_LOBBY_BOTS_0_ENABLED=true

# Bot 2 Configuration (optional)
DOTA_LOBBY_BOTS_1_NAME=bot2
DOTA_LOBBY_BOTS_1_USERNAME=your_steam_username_2
DOTA_LOBBY_BOTS_1_PASSWORD=your_steam_password_2
DOTA_LOBBY_BOTS_1_ENABLED=false
